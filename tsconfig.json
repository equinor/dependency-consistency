{
    "compilerOptions": {
        "target": "ESNext",
        "module": "CommonJS",
        "lib": ["ESNext", "DOM"],
        "types": ["node", "semver-sort"],
        "moduleResolution": "Node",
        "isolatedModules": false,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}
