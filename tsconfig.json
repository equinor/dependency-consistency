{
    "files": [
        "index.cjs",
        "scripts/keep-own-dependencies-up-to-date.cjs",
        "eslint.config.cjs",
    ],
    "compilerOptions": {
        "target": "ESNext",
        "module": "CommonJS",
        "lib": ["ESNext", "DOM"],
        "types": ["node", "semver-sort"],
        "moduleResolution": "Node",
        "allowJs": true,
        "checkJs": true,
        "noEmit": true,
        "isolatedModules": false,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "forceConsistentCasingInFileNames": true
    }
}
